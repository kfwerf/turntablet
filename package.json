{
  "name": "Turntablet",
  "version": "0.0.1",
  "description": "ES6 based app that allows you to manipulate music like a dj via touch enabled turntable-like controls",
  "main": "app/app.js",
  "scripts": {
    "start": "nf start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "./node_modules/.bin/serve -p 8000",
    "webpack": "env UV_THREADPOOL_SIZE=100 ./node_modules/.bin/webpack --display-error-details --watch --stats --progress --config ./app.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kfwerf/turntablet"
  },
  "keywords": [
    "turntablet",
    "touch",
    "es6",
    "babel",
    "webaudio"
  ],
  "author": "Kenneth van der Werf",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kfwerf/turntablet/issues"
  },
  "homepage": "https://github.com/kfwerf/turntablet",
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-loader": "^5.3.2",
    "baggage-loader": "^0.2.4",
    "foreman": "^1.4.1",
    "id3-parser": "^1.0.1",
    "loader-utils": "^0.2.11",
    "serve": "^1.4.0",
    "source-map": "^0.1.43",
    "webpack": "^1.12.2"
  }
}
